            aZ.Position = UDim2.new(0, 5, 0, 0)
            aZ.BackgroundTransparency = 1
            aZ.Text = aT.CurrentText or ""
            aZ.PlaceholderText = aT.PlaceholderText or "Enter text..."
            aZ.Font = Enum.Font.Gotham
